// 遍历文档中的所有文本框并替换字符
for (var i = 0; i < textFrames.length; i++) {
    var text = textFrames[i].contents;

    // 循环映射表，将错误的韩文字符替换为中文字符
    for (var key in mapping) {
        var koreanChar = mapping[key];
        var chineseChar = key;
        // 使用正则表达式进行全局替换
        text = text.replace(new RegExp(koreanChar, 'g'), chineseChar);
    }

    // 将替换后的文本重新赋值给文本框
    textFrames[i].contents = text;
}

alert("字符替换完成！");
